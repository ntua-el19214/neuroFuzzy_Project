function delta = deltaFunc(t)
    
if t < 0.5
    delta = 0;
elseif t < 1.5
    phase = 2 * pi / 1.5 * 0.5;
    delta = -deg2rad(10) * sin(2 * pi / 1 * t - phase);
elseif t < 4
    phase = 2 * pi / 2.5 * 1;
    delta = deg2rad(20) * sin(2 * pi / 1.6 * t - phase);
elseif t < 3.1
    phase = 2 * pi / 1.5 * 2.6;
    delta = deg2rad(10) * sin(2 * pi / 1 * t - phase);
else
    delta = 0;


end
